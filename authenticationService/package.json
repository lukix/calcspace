{
  "name": "calcspace-authentication-service",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node out/devServerAdapter.js",
    "dev": "concurrently \"tsc --watch\" \"nodemon --watch out out/devServerAdapter.js\"",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint \"./src/*\"",
    "format:check": "prettier --check ./src/*",
    "format:fix": "prettier --write ./src/*",
    "pre-push": "yarn run format:check && yarn run lint && CI=true yarn run test",
    "pre-commit": "pretty-quick --staged",
    "setupDb": "node ./out/setupDatabase.js"
  },
  "dependencies": {
    "aws-sdk": "^2.1213.0",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "concurrently": "^5.1.0",
    "eslint": "^7.23.0",
    "eslint-plugin-jest": "^24.3.2",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "~2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": "14.16.x"
  }
}
